@{
}
@model ESTA.ViewModels.DisplayEventObj
@{
    if (Model.Events.Count > 0)
    {
        await Html.RenderPartialAsync("_renderEvent", new ESTA.ViewModels.DisplayEventPartial()
                {
                    DivId = "Event",
                    Events = Model.Events
                });
    }
    if (Model.News.Count > 0)
    {
        await Html.RenderPartialAsync("_renderEvent", new ESTA.ViewModels.DisplayEventPartial()
                {
                    DivId = "News",
                    Events = Model.News
                });
    }
}
