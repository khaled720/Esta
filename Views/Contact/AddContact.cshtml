@model ESTA.Models.Contact

<h2>Add New Contact</h2>

<form method="post" >

    <label asp-for="@Model.AddressEn" ></label>
<input asp-for="@Model.AddressEn" class="form-control" />

    <label asp-for="@Model.AddressAr" ></label>
<input asp-for="@Model.AddressAr" class="form-control" />

    <label asp-for="@Model.PhoneLines" ></label>
<input asp-for="@Model.PhoneLines" class="form-control" />

 <label asp-for="@Model.Emails" ></label>
<input asp-for="@Model.Emails" class="form-control" />

    <label asp-for="@Model.TitleAr" ></label>
<input asp-for="@Model.TitleAr" class="form-control" />

    <label asp-for="@Model.TitleEn" ></label>
<input asp-for="@Model.TitleEn" class="form-control" />

<button type="submit" class="btn-primary btn">Submit</button>
</form>