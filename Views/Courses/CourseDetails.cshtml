@if (Model != null)
{
    <div id="choice" style="z-index:2; display:none;backdrop-filter:blur(5px);position:absolute;
top:0;bottom:0;right:0;left:0 ">


        <div style="z-index: 100000;background: white;
    padding: 32px;left: 30%;top: 30%;position: sticky;width: 50%;
    border: 0.5px gray solid;border-radius: 15px;">

            <p>@localizer["coursepopup"] </p>

            @if (!await uow.UserRep.IsForeignUser(Model.userid) 
            && Model.course.StartDate!=null
            && ( Model.course.StartDate-DateTime.Now).Value.Days >= 0 )
            {

            <a asp-action="PayEnrollCourse" asp-controller="User"
           asp-route-id="@Model.course.Id" asp-route-level="@Model.course.LevelId"
            class="btn btn-danger"> @localizer["paynow"]</a>

            }else{
            
                 @if (await uow.UserRep.IsForeignUser(Model.userid)
         && Model.course.StartDate != null
         //&& (Model.StartDate - DateTime.Now).Value.Days < 0
               && (Model.course.StartDate - DateTime.Now).Value.Days < 10
                 )
            { 
            
            <p class="text-danger">Enrollment Not Available right now ,If you want to register <a class="btn btn-primary" asp-controller="Home" asp-action="Contact" asp-area="">Contact Us</a></p>
            
            }

            }


            @if (Model.course.StartDate!=null&&(Model.course.StartDate-DateTime.Now  ).Value.Days >= 10)
            {

             <a id="enroll" asp-action="EnrollCourse" asp-controller="User"
           asp-route-Id="@Model.course.Id" asp-route-level="@Model.course.LevelId"
            class="btn btn-warning"> @localizer["paylater"]</a>   

            }

       

            <a onclick="HideEnrollOptions()"  href="#" class=""> @localizer["cancel"]</a>

        </div>

    </div>
}




