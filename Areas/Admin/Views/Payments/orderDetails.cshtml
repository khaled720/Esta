@model dynamic

@{
    ViewData["Title"] = "orderDetails";
}

<h1>orderDetails</h1>

<div>
    <h4>Order</h4>
    <hr />
    <dl class="row">
 
    
        <dt class = "col-sm-2">
            Creation Time
        </dt>
        <dd class = "col-sm-10">
          @Model.CreationTime
        </dd>
        <dt class = "col-sm-2">
            Order Number
        </dt>
        <dd class = "col-sm-10">
       @Model.OrderNumber
        </dd>

        <dt class = "col-sm-2">
           Amount
        </dt>
        <dd class = "col-sm-10">
         @Model.Amount
          @Model.Currency
        </dd>
     
        <dt class = "col-sm-2">
            Order Reference
        </dt>
        <dd class = "col-sm-10">
       @Model.OrderReference
        </dd>
        <dt class = "col-sm-2">
            Transaction Reference
        </dt>
        <dd class = "col-sm-10">
          @Model.TransactionReference
        </dd>
      
        <dt class = "col-sm-2">
            Order Result
        </dt>
        <dd class = "col-sm-10">
    @Model.OrderResult
        </dd>
    
           <dt class = "col-sm-2">
            Order Description
        </dt>
        <dd class = "col-sm-10">
       @Model.OrderDescription
        </dd>
  
    </dl>
</div>

